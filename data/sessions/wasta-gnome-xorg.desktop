[Desktop Entry]
Name=Wasta GNOME on Xorg
Comment=This session logs you into Wasta GNOME
Exec=env GNOME_SHELL_SESSION_MODE=wasta /usr/bin/gnome-session --session=ubuntu
TryExec=/usr/bin/gnome-shell
Type=Application
DesktopNames=wasta-gnome:ubuntu:GNOME
X-GDM-SessionRegisters=true
X-Ubuntu-Gettext-Domain=gnome-session-45
